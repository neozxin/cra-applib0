    "jest-image-snapshot": "^3.0.1",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "puppeteer": "^2.1.1",
    "react-app-rewired": "^2.1.5",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.10.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
